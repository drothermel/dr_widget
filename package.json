{
  "name": "dr_widget",
  "version": "0.1.0",
  "workspaces": ["src/dr_widget/widgets/*"],
  "scripts": {
    "build": "bun run build:all",
    "build:all": "bun run build:config-file-manager",
    "build:config-file-manager": "bun --cwd src/dr_widget/widgets/config_file_manager vite build",
    "dev": "bun run dev:config-file-manager",
    "dev:config-file-manager": "bun --cwd src/dr_widget/widgets/config_file_manager vite dev",
    "preview:config-file-manager": "bun --cwd src/dr_widget/widgets/config_file_manager vite preview"
  },
  "module": "index.ts",
  "type": "module",
  "private": true,
  "dependencies": {
    "@anywidget/svelte": "^0.1.0",
    "svelte": "^5.43.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
